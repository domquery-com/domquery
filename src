                a(c)
            }))
        }
        a(e);
        a(l);
        e.addEventListener("mouseover", function(b) {
            b.target && b.target.removeAttribute("title")
        }, !0)
    }

    function G() {
        if ("undefined" !==
            typeof $ && $.isMobile) {
            const a = $.isMobile();
            return !0 === a.isMobile || !0 === a.isTablet
        }
        return !1
    }

