                },
                !1)) : (c.className = "fixed-color", c.style.backgroundColor = a, c.setAttribute("data-color", a), c.addEventListener("click", function(f) {
                f.preventDefault();
                f.stopPropagation();
                f = this.getAttribute("data-color");
                ia(f);
                G() && h.keepKeyboard && setTimeout(() => z.focus(), 0);
                this.focus()
            }, !1));
