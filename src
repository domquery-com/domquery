        y: d
    });
    this.data("svgPoints", l);
    this.animate = function(m, p, r, n) {
        "function" === typeof p ? (n = p, p = 400, r = "swing") : "function" === typeof r && (n = r, r = "swing");
        p = p || 400;
        r = r || "swing";
        if (void 0 !== m.left && void 0 !== m.top) {
            const z = this.data("svgPoints");
            z.push({
                x: parseFloat(m.left),
                y: parseFloat(m.top),
                duration: p,
                easing: r
            });
