    function ua(a, b, c) {
        let f, k, m, n = Math.floor(a / 60) % 6,
            V = a / 60 - n;
        a = c * (1 - b);
        let I = c * (1 - V * b);
        b = c * (1 - (1 - V) * b);
        switch (n) {
            case 0:
                f = c;
                k = b;
                m = a;
                break;
            case 1:
                f = I;
                k = c;
                m = a;
                break;
            case 2:
                f = a;
                k = c;
                m = b;
                break;
            case 3:
                f = a;
                k = I;
                m = c;
                break;
            case 4:
                f = b;
                k = a;
                m = c;
                break;
            case 5:
                f = c, k = a, m = I
        }
        return {
            r: Math.round(255 * f),
            g: Math.round(255 * k),
            b: Math.round(255 * m)
        }
    }

    function J(a, b, c) {
        a = Math.max(0, Math.min(255, Math.round(a)));
        b = Math.max(0, Math.min(255, Math.round(b)));
        c = Math.max(0, Math.min(255, Math.round(c)));
        return "#" + a.toString(16).padStart(2, "0") + b.toString(16).padStart(2, "0") + c.toString(16).padStart(2, "0")
    }

    function ma(a) {
        if ("transparent" === a) return {
            r: 0,
            g: 0,
            b: 0,
            a: 0
        };
        a = a.replace("#", "");
        3 === a.length && (a = a[0] + a[0] + a[1] + a[1] + a[2] + a[2]);
        if (!/^[0-9A-Fa-f]{6}$/.test(a)) return {
            r: 0,
            g: 0,
            b: 0
        };
        try {
            return {
                r: parseInt(a.substring(0, 2), 16),
                g: parseInt(a.substring(2, 4), 16),
                b: parseInt(a.substring(4, 6), 16)
            }
        } catch (b) {
            return console.warn("Kcolor: \uc0c9\uc0c1 \ubcc0\ud658 \uc624\ub958",
                b), {
                r: 0,
                g: 0,
                b: 0
            }
        }
    }

    function na(a, b, c) {
        a /= 255;
        b /= 255;
        c /= 255;
        const f = Math.max(a, b, c),
            k = Math.min(a, b, c);
        let m;
        const n = f - k;
        if (f === k) m = 0;
        else {
            switch (f) {
                case a:
                    m = (b - c) / n + (b < c ? 6 : 0);
                    break;
                case b:
                    m = (c - a) / n + 2;
                    break;
                case c:
                    m = (a - b) / n + 4
            }
            m /= 6
        }
        return {
            h: Math.round(360 * m),
            s: 0 === f ? 0 : n / f,
            v: f
        }
    }

